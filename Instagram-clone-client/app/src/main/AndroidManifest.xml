<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pershay.bstu.woolfy">
    <!-- Include following permission if you load images from Internet -->
    <uses-permission android:name="android.permission.INTERNET" /> <!-- Include following permission if you want to cache images on SD card -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_PERMISSION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".security.EnterPasswordActivity"></activity>
        <activity android:name=".security.CreatePasswordActivity" />
        <activity android:name=".UserProfileActivity" />
        <activity android:name=".profile.EditProfileActivity" />
        <activity android:name=".login.RegistrationUserDataActivity" />
        <activity android:name=".login.RegistrationUserActivity" />
        <activity android:name=".login.LoginActivity" />
        <activity android:name=".addphoto.NextActivity" /> <!-- <activity android:name=".login.LoginActivity" /> -->
        <activity android:name=".profile.ProfileActivity" />
        <activity android:name=".addphoto.AddPhotoActivity" />
        <activity android:name=".main.MainActivity" />
        <activity android:name=".security.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>